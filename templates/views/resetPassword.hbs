<!doctype html>
<html lang="en">


<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>TrignoCloud || {{Title}}</title>

    <!-- Favicon -->
    <link rel="shortcut icon" type="text/css" href="/images/favicon.ico" />

    <link rel="stylesheet" type="text/css" href=" css/style.css">
    <link rel="stylesheet" type="text/css" href="vendor/@fortawesome/fontawesome-free/css/all.min.css">
    <link rel="stylesheet" type="text/css" href="/vendor/line-awesome/dist/line-awesome/css/line-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="/vendor/remixicon/fonts/remixicon.css">

    <!-- Viewer Plugin -->
    <!--PDF-->
    <link rel="stylesheet" type="text/css" href="vendor/doc-viewer/include/pdf/pdf.viewer.css">
    <!--Docs-->
    <!--PPTX-->
    <link rel="stylesheet" type="text/css" href="/vendor/doc-viewer/include/PPTXjs/css/pptxjs.css">
    <link rel="stylesheet" type="text/css" href="/vendor/doc-viewer/include/PPTXjs/css/nv.d3.min.css">
    <!--All Spreadsheet -->
    <link rel="stylesheet" type="text/css" href="/vendor/doc-viewer/include/SheetJS/handsontable.full.min.css">
    <!--Image viewer-->
    <link rel="stylesheet" type="text/css"
        href="/vendor/doc-viewer/include/verySimpleImageViewer/css/jquery.verySimpleImageViewer.css">
    <!--officeToHtml-->
    <link rel="stylesheet" type="text/css" href="/vendor/doc-viewer/include/officeToHtml/officeToHtml.css">
</head>


<body class=" ">
    <!-- loader Start -->
    <div id="loading">
        <div id="loading-center">
        </div>
    </div>
    <!-- loader END -->

    <div class="wrapper">
        <section class="login-content">
            <div class="container h-100">
                <div class="row justify-content-center align-items-center height-self-center">
                    <div class="col-md-5 col-sm-12 col-12 align-self-center m-0">
                        <div class="sign-user_card" herf="/trignoweb">
                            <img src="/images/favicon.ico " class="img-fluid rounded-normal light-logo logo" alt="logo">
                            <img src="/images/logo-white.png" class="img-fluid rounded-normal darkmode-logo logo"
                                alt="logo">
                            <h3 class="mb-3">Froget Password</h3>
                            <p>Create a new password.</p>
                            <form action="" method="post">
                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="floating-label form-group">
                                            <input class="floating-input form-control" type="password" placeholder=" "
                                                name="password" value="">
                                            <label>Password</label>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="floating-label form-group">
                                            <input class="floating-input form-control" type="password" placeholder=" "
                                                name="confirmpassword" value="">
                                            <label>Confirm Password</label>
                                        </div>
                                    </div>
                                </div>
                                <button type="submit" class="btn btn-primary" value="">Submit</button>

                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>


    <!-- Backend Bundle JavaScript -->

    <script src="/js/backend-bundle.min.js"></script>

    <!-- Chart Custom JavaScript -->
    <script src="/js/customizer.js"></script>

    <!-- Chart Custom JavaScript -->
    <script src="/js/chart-custom.js"></script>

    <!--PDF-->
    <script src="/vendor/doc-viewer/include/pdf/pdf.js"></script>
    <!--Docs-->
    <script src="/vendor/doc-viewer/include/docx/jszip-utils.js"></script>
    <script src="/vendor/doc-viewer/include/docx/mammoth.browser.min.js"></script>
    <!--PPTX-->
    <script src="/vendor/doc-viewer/include/PPTXjs/js/filereader.js"></script>
    <script src="/vendor/doc-viewer/include/PPTXjs/js/d3.min.js"></script>
    <script src="/vendor/doc-viewer/include/PPTXjs/js/nv.d3.min.js"></script>
    <script src="/vendor/doc-viewer/include/PPTXjs/js/pptxjs.js"></script>
    <script src="/vendor/doc-viewer/include/PPTXjs/js/divs2slides.js"></script>
    <!--All Spreadsheet -->
    <script src="/vendor/doc-viewer/include/SheetJS/handsontable.full.min.js"></script>
    <script src="/vendor/doc-viewer/include/SheetJS/xlsx.full.min.js"></script>
    <!--Image viewer-->
    <script src="/vendor/doc-viewer/include/verySimpleImageViewer/js/jquery.verySimpleImageViewer.js"></script>
    <!--officeToHtml-->
    <script src="/vendor/doc-viewer/include/officeToHtml/officeToHtml.js"></script>

    <script src="/js/doc-viewer.js"></script>
    <!-- app JavaScript -->
    <script src="/js/app.js"></script>
</body>

</html>