<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title>TrignoCloud || {{Title}}</title>

  <!-- Favicon -->
  <link rel="shortcut icon" type="text/css" href="/images/favicon.ico" />

  <link rel="stylesheet" type="text/css" href=" css/style.css">
  <link rel="stylesheet" type="text/css" href="/vendor/@fortawesome/fontawesome-free/css/all.min.css">
  <link rel="stylesheet" type="text/css" href="/vendor/line-awesome/dist/line-awesome/css/line-awesome.min.css">
  <link rel="stylesheet" type="text/css" href="/vendor/remixicon/fonts/remixicon.css">

  <!-- Viewer Plugin -->
  <!--PDF-->
  <link rel="stylesheet" type="text/css" href="vendor/doc-viewer/include/pdf/pdf.viewer.css">
  <!--Docs-->
  <!--PPTX-->
  <link rel="stylesheet" type="text/css" href="/vendor/doc-viewer/include/PPTXjs/css/pptxjs.css">
  <link rel="stylesheet" type="text/css" href="/vendor/doc-viewer/include/PPTXjs/css/nv.d3.min.css">
  <!--All Spreadsheet -->
  <link rel="stylesheet" type="text/css" href="/vendor/doc-viewer/include/SheetJS/handsontable.full.min.css">
  <!--Image viewer-->
  <link rel="stylesheet" type="text/css"
    href="/vendor/doc-viewer/include/verySimpleImageViewer/css/jquery.verySimpleImageViewer.css">
  <!--officeToHtml-->
  <link rel="stylesheet" type="text/css" href="/vendor/doc-viewer/include/officeToHtml/officeToHtml.css">
</head>


<body class=" ">
  <!-- loader Start -->
  <div id="loading">
    <div id="loading-center">
    </div>
  </div>
  <!-- loader END -->

  <div class="wrapper">
    <section class="login-content">
      <div class="container h-100">
        <div class="row justify-content-center align-items-center height-self-center">
          <div class="col-md-5 col-sm-12 col-12 align-self-center">
            <div class="sign-user_card">
              <img src="/images/favicon.ico" class="img-fluid rounded-normal light-logo logo" alt="logo">
              <img src="/images/logo-white.png" class="img-fluid rounded-normal darkmode-logo logo" alt="logo">
              <h3 class="mb-3">Sign Up</h3>
              <p>Create your account.</p>
              <form action="/signup" method="post">
                <div class="row">
                  <div class="col-lg-6">
                    <div class="floating-label form-group">
                      <input class="floating-input form-control" type="text" placeholder=" " name="firstname" value=""
                        autocomplete="off">
                      <label>FIsrt Name</label>
                    </div>
                  </div>
                  <div class="col-lg-6">
                    <div class="floating-label form-group">
                      <input class="floating-input form-control" type="text" placeholder="" name="lastname" value=""
                        autocomplete="off">
                      <label>Last Name</label>
                    </div>
                  </div>
                  <div class="col-lg-12">
                    <div class="floating-label form-group">
                      <input class="floating-input form-control" type="email" placeholder=" " name="email" value=""
                        autocomplete="off">
                      <label>Email</label>
                    </div>
                  </div>
                  <div class="col-lg-6">
                    <div class="floating-label form-group">
                      <input class="floating-input form-control" type="password" placeholder=" " name="password"
                        value="">
                      <label>Password</label>
                    </div>
                  </div>
                  <div class="col-lg-6">
                    <div class="floating-label form-group">
                      <input class="floating-input form-control" type="password" placeholder=" " name="confirmpassword"
                        value="">
                      <label>Confirm Password</label>
                    </div>
                  </div>
                  {{!-- <div class="col-lg-12">
                    <div class="custom-control custom-checkbox mb-3 text-left">
                      <input type="checkbox" class="custom-control-input" id="customCheck1">
                      <label class="custom-control-label" for="customCheck1">I agree with the terms of
                        use</label>
                    </div>
                  </div> --}}
                </div>
                <button type="submit" class="btn btn-primary ">Sign Up</button>
                <p class="">
                  Already have an Account <a href="./signin" class="text-primary">Sign In</a>
                </p>
              </form>
            </div>
          </div>
        </div>
      </div>
    </section>
  </div><!-- Backend Bundle JavaScript -->
  <script src="/js/backend-bundle.min.js" type="text/javascript"></script>

  <!-- Chart Custom JavaScript -->
  <script src="/js/customizer.js" type="text/javascript"></script>

  <!-- Chart Custom JavaScript -->
  <script src="/js/chart-custom.js" type="text/javascript"></script>

  <!--PDF-->
  <script src="/vendor/doc-viewer/include/pdf/pdf.js" type="text/javascript"></script>
  <!--Docs-->
  <script src="/vendor/doc-viewer/include/docx/jszip-utils.js" type="text/javascript"></script>
  <script src="/vendor/doc-viewer/include/docx/mammoth.browser.min.js" type="text/javascript"></script>
  <!--PPTX-->
  <script src="/vendor/doc-viewer/include/PPTXjs/js/filereader.js" type="text/javascript"></script>
  <script src="/vendor/doc-viewer/include/PPTXjs/js/d3.min.js" type="text/javascript"></script>
  <script src="/vendor/doc-viewer/include/PPTXjs/js/nv.d3.min.js" type="text/javascript"></script>
  <script src="/vendor/doc-viewer/include/PPTXjs/js/pptxjs.js" type="text/javascript"></script>
  <script src="/vendor/doc-viewer/include/PPTXjs/js/divs2slides.js" type="text/javascript"></script>
  <!--All Spreadsheet -->
  <script src="/vendor/doc-viewer/include/SheetJS/handsontable.full.min.js" type="text/javascript"></script>
  <script src="/vendor/doc-viewer/include/SheetJS/xlsx.full.min.js" type="text/javascript"></script>
  <!--Image viewer-->
  <script src="/vendor/doc-viewer/include/verySimpleImageViewer/js/jquery.verySimpleImageViewer.js"
    type="text/javascript"></script>
  <!--officeToHtml-->
  <script src="/vendor/doc-viewer/include/officeToHtml/officeToHtml.js" type="text/javascript"></script>
  <script src="/js/doc-viewer.js type=" text/javascript""></script>
  <!-- app JavaScript -->
  <script src="/js/app.js" type="text/javascript"></script>
  <!-- Modal -->

</body>

</html>